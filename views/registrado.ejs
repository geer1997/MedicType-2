<main class="grey lighten-4">
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-registrar">
      <h2 class="white-text accent">Paciente</h2>
    </div>
  </div>
 

  <div class="container">
    <div class="row card-panel">
      <form disabled class="col s12">
        <div class="row">
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Nombre %>" name="Nombre" type="text" class="validate">
            <label for="Nombre">Nombre Paciente</label>
          </div>
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Apellido %>" name="Apellido" type="text" class="validate">
            <label for="Apellido">Apellido Paciente</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Cedula %>" name="Cedula" type="text" class="validate">
            <label for="Cedula">Cédula</label>
          </div>
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Sexo %>" name="Sexo" type="text" class="validate">
            <label for="Sexo">Sexo</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.TSangre %>" name="TSangre" type="text" class="validate">
            <label for="TSangre">Tipo de Sangre</label>
          </div>
          <div class="col s6">
            <label for="FNacimiento">Fecha de Nacimiento</label>
            <input disabled value="<%= Paciente.FNacimiento %>" type="text" class="validate" name="FNacimiento">
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.NAptoCasa %>" name="NAptoCasa" type="text" class="validate">
            <label for="NAptoCasa">N° de Apto / Casa</label>
          </div>
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Urbanizacion %>" name="Urbanizacion" type="text" class="validate">
            <label for="Urbanizacion">Urbanización</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Calle %>" name="Calle" type="text" class="validate">
            <label for="Calle">Calle</label>
          </div>
          <div class="col s6 input-field">
            <input disabled value="<%= Paciente.Ciudad %>" name="Ciudad" type="text" class="validate">
            <label for="Ciudad">Municipio / Ciudad</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <textarea disabled name="alergiasP" class="materialize-textarea"></textarea>
            <label for="alergiaP">Alergia</label>
          </div>
          <div class="col s6 input-field">
            <textarea disabled name="antecentesF" class="materialize-textarea"></textarea>
            <label for="antecentesF">Antecedentes Familiares</label>
          </div>
        </div>

      </form>


      <div class="row">
        <div class="col s2 offset-s4 ">
          <a href="/paciente/Modificar/<%= Paciente.id %>" class="waves-effect waves-light btn blue darken-1">Editar datos</a>
        </div>
        <div class="col s2">
        <form method="POST" action="/paciente/eliminar/<%= Paciente.id %>">
          <button type="submit" class="waves-effect waves-light btn blue darken-1">Eliminar</button>
        </form>
      </div>
    </div>
  </div>
  </div>

</main>