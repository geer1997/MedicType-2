<main class="grey lighten-4">
        <div class="row">
          <div class="col m12 valign-wrapper" id="header-registrar">
            <h2 class="white-text accent">Modificar Paciente</h2>
          </div>
        </div>

<form method="POST" action="/paciente/update/<%= Paciente.id %>" class="container">
    <div class="row card-panel">
      <div class="col s12">

        <div class="row">
          <div class="col s6 input-field">
            <input value="<%= Paciente.Nombre %>" name="Nombre" type="text" class="validate">
            <label for="Nombre">Nombre Paciente</label>
          </div>
          <div class="col s6 input-field">
            <input value="<%= Paciente.Apellido %>" name="Apellido" type="text" class="validate">
            <label for="Apellido">Apellido Paciente</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input value="<%= Paciente.Cedula %>" type="text" class="validate">
            <label for="Cedula">Cédula</label>
          </div>
          <div class="col s6 input-field">
            <input value="<%= Paciente.Sexo %>" name="Sexo" type="text" class="validate">
            <label for="Sexo">Sexo</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input value="<%= Paciente.TSangre %>" name="TSangre" type="text" class="validate">
            <label for="TSangre">Tipo de Sangre</label>
          </div>
          <div class="col s6 input-field">
              <input value="<%= Paciente.FNacimiento %>" type="text" class="datepicker" name="FNacimiento" />
              <label for="FNacimiento">Fecha de Nacimiento</label>
            </div>
        </div>
     
        <div class="row">
          <div class="col s6 input-field">
            <input value="<%= Paciente.NAptoCasa %>" name="NAptoCasa" type="text" class="validate" />
            <label for="NAptoCasa">N° de Apto / Casa</label>
          </div>
          <div class="col s6 input-field">
            <input value="<%= Paciente.Urbanizacion %>" type="text" class="validate" />
            <label for="Urbanizacion">Urbanización</label>
          </div>
        </div>

        <div class="row">
          <div class="col s6 input-field">
            <input value="<%= Paciente.Calle %>" name="Calle" type="text" class="validate" />
            <label for="Calle">Calle</label>
          </div>
          <div class="col s6 input-field">
            <input value="<%= Paciente.Ciudad %>" name="Ciudad" type="text" class="validate" />
            <label for="Ciudad">Municipio / Ciudad</label>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col s12 center">
          <button class="waves-effect waves-light btn blue darken-1" type="submit">Guardar</button>
        </div>
      </div>

    </div>

  </form>

</main>