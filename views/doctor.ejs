<main class="grey lighten-5">
  <div class="fixed-action-btn">
    <a class="btn-floating btn-large red">
      <i class="large material-icons">add</i>
    </a>
    <ul>
      <li>
        <a class="btn-floating red" href="/paciente/registrar">
          <i class="material-icons">fiber_new</i>
        </a>
      </li>
      <li>
        <a href="#modalBuscar" id="btnBuscar" class="btn-floating yellow darken-1 modal-trigger">
          <i class="material-icons">search</i>
        </a>
      </li>
      <li>
        <a href="/doctor/panel" id="btnAtender" class="btn-floating blue darken-1">
          <i class="material-icons">done</i>
        </a>
      </li>
    </ul>
  </div>
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-doctor">
      <h2 class="white-text"><span class="center">Consulta del Dr. <span id="doctor accent"><%= medico.Apellido %></span></span></h2>
    </div>
  </div>


  <div class="container">
    <div class="row">
      <div class="col m12 card-panel hoverable">
        <h5></h5>
        <table class="highlight responsive-table">
          <thead>
            <tr>
              <th>Cédula</th>
              <th>Nombre</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% pacientes.forEach(function () { %>
              <tr>
                <td> <%= paciente.Cedula%> </td>
                <td> <%= paciente.Nombre + paciente.Apellido%> </td>
                <td></td>
              </tr>
            <% })%>
          </tbody>
        </table>
      </div>
    </div>
  </div>



  <form id="modalBuscar" class="modal" method="POST" action="/paciente/BuscarPorCedula">
    <div class="modal-content">
      <div class="row">
        <h4 class="center">Buscar Paciente por Numero de Cedula</h4>
      </div>
      <div class="row">
        <div class="col m12 input-field">
          <input placeholder="12345678" name="Cedula" id="Cedula" type="text" class="validate">
          <label for="Cedula">Cédula de Identidad</label>
        </div>
      </div>
    </div>
    <div class="modal-footer">
      <button class="modal-action modal-close waves-effect waves-green btn-flat " type="submit" id="btnBuscar">Buscar</button>
    </div>
  </form>
</main>
<script>


</script>
