<main class="grey lighten-4">
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-registrar">
      <h2 class="white-text accent">Datos Personales del Paciente</h2>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col m3"></div>
      <div class="col m6 card-panel">
        <div class="row">
          <h4 class="center">Buscar Paciente</h4>
        </div>
        <form method="POST" action="/paciente/BuscarPorCedula" class="row"> 
          <div class="col m12 center input-field">
            <input placeholder="12345678" name="Cedula" type="text" class="validate">
            <label for="Cedula">Cedula de Identidad</label>
            <button class="waves-effect waves-light btn blue darken-1" type="submit">Buscar</button>
          </div>
        </form>
      </div>
      <div class="col m3"></div>
    </div>
  </div>


</main>