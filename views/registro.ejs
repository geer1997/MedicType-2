<main class="grey lighten-4">
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-registrar">
      <h2 class="white-text accent">Registrar Nuevo Paciente</h2>
    </div>
  </div>
  <form method="POST" action="/paciente/crear" class="container">
    <div class="row card-panel">
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Pedro" name="Nombre" type="text" class="validate">
            <label for="Nombre">Nombre Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Perez" name="Apellido" type="text" class="validate">
            <label for="Apellido">Apellido Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="12345678" name="Cedula" type="text" class="validate">
            <label for="Cedula">Cédula</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="AB ORH+" name="TSangre" type="text" class="validate">
            <label for="TSangre">Tipo de Sangre</label>
          </div>
        </div>

       <!-- 
        <div class="row">
          <div class="col s12 input-field">
            <textarea name="alergiasP" class="materialize-textarea"></textarea>
            <label for="alergiaP">Alergia</label>
          </div>
        </div>
      

        <div class="row">
          <div class="col s12 input-field">
            <textarea id="antecentesF" class="materialize-textarea"></textarea>
            <label for="antecentesF">Antecedentes Familiares</label>
          </div>
        </div>-->
      </div>
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Apto. XX Edif. xxxx / Casa N° x" name="NAptoCasa" type="text" class="validate" />
            <label for="NAptoCasa">N° de Apto / Casa</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Urb. XXXXXX" name="Urbanizacion" type="text" class="validate" />
            <label for="Urbanizacion">Urbanización</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Calle xx / Av xx" name="Calle" type="text" class="validate" />
            <label for="Calle">Calle</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Municipio Sucre / Caracas" name="Ciudad" type="text" class="validate" />
            <label for="Ciudad">Municipio / Ciudad</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input type="text" class="datepicker" name="FNacimiento" />
            <label for="FNacimiento">Fecha de Nacimiento</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Femenino/masculino" name="Sexo" type="text" class="validate">
            <label for="Sexo">Sexo</label>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col s12 center">
          <input class="waves-effect waves-light btn blue darken-1" type="submit" value="Registrar" />
        </div>
      </div>
    </div>
  </form>
</main>
<footer class="blue darken-1 page-footer">
  <div class="row">
    <div class="col s12">
      <p class="center white-text"> © 2017 <span class="accent">MedicType Team.</span></p>
    </div>
  </div>
</footer>
