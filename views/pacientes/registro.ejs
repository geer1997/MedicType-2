<main class="grey lighten-4">
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-registrar">
      <h2 class="white-text accent">Registrar Nuevo Paciente</h2>
    </div>
  </div>
  <form method="POST" action="/Paciente/create" class="container">
    <div class="row card-panel">
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Pedro" id="namePaciente" type="text" class="validate">
            <label for="namePaciente">Nombre Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Perez" id="apellidoPaciente" type="text" class="validate">
            <label for="apellidoPaciente">Apellido Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="12345678" id="cedulaPaciente" type="text" class="validate">
            <label for="cedulaPaciente">Cédula</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="AB ORH+" id="tipoSangre" type="text" class="validate">
            <label for="tipoSangre">Tipo de Sangre</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <textarea id="alergiasP" class="materialize-textarea"></textarea>
            <label for="alergiaP">Alergia</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <textarea id="antecentesF" class="materialize-textarea"></textarea>
            <label for="antecentesF">Antecedentes Familiares</label>
          </div>
        </div>
      </div>
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Apto. XX Edif. xxxx / Casa N° x" id="namePaciente" type="text" class="validate">
            <label for="namePaciente">N° de Apto / Casa</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Urb. XXXXXX" id="apellidoPaciente" type="text" class="validate">
            <label for="apellidoPaciente">Urbanización</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Calle xx / Av xx" id="cedulaPaciente" type="text" class="validate">
            <label for="cedulaPaciente">Calle</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Municipio Sucre / Caracas" id="tipoSangre" type="text" class="validate">
            <label for="tipoSangre">Municipio / Ciudad</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input type="text" class="datepicker" id="fechaNacimiento">
            <label for="fechaNacimiento">Fecha de Nacimiento</label>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col s12 center">
          <button class="waves-effect waves-light btn blue darken-1" type="summit">Registrar</button>
        </div>
      </div>
    </div>
  </div>
</main>
<footer class="blue darken-1 page-footer">
  <div class="row">
    <div class="col s12">
      <p class="center white-text"> © 2017 <span class="accent">MedicType Team.</span></p>
    </div>
  </div>
</footer>
